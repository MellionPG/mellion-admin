<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mellion.admin.system.mapper.TSystemDictItemMapper">
    <select id="getItemValueAndTextByDefineId" resultType="java.util.Map">
        select value, text
        from t_system_dict_item
        where define_id = #{defineId}
    </select>
</mapper>